
using x, y of type integer; c of type canvas begin
x := 10 ;
y := 15 ;
c := <empty> ;
{-hala-}
r := <_>
r < 4
from 1 to x repeat
using d of type canvas begin
d := <empty> ;
with j from 1 to y repeat
if j % 2 = 0 then
d := d : </>
else
d := d : <\>
done
done ;
c := c | d
end
done ;
print c
print true
{-====!===-}
using x, y of type interger; c of type canvas begin
   x := 10 ;{-ghsjkg
fdhsajkhfdsa
hfdjskafdsa
hjkfdsa	gfjs
-}{-gdsag
-}
   y := 15 ;
   c := <empty> ;
   from 1 to x repeat
     using d of type canvas begin
	d := <empty> ;
	with j from 1 to y repeat
	   if j % 2 = 0 then
		d := d : </>
	   else
		d := d : <\>
	   done
	done ;
	c := c | d
     end
   done ;
   print{-gjsklg
	gfdjkshgfs
	gskj
	fdsa


						-} c
end
of type
